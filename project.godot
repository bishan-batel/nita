[application]

config/name="Parry2"
run/main_scene="res://src/menu/Menu.tscn"
boot_splash/image="res://assets/textures/detail/backgrounds/mushroom_plains 0.png"
config/icon="res://assets/textures/objects/saw/saw.png"

[autoload]

Global="*res://src/Global.cs"
GameStateManager="*res://src/managers/game/GameStateManager.cs"
DialogicSingleton="*res://addons/dialogic/Other/DialogicSingleton.gd"
SaveManager="*res://src/managers/save/SaveManager.cs"
SoundManager="*res://src/managers/sound/SoundManager.cs"

[display]

window/size/width=286
window/size/height=168
window/dpi/allow_hidpi=true
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor]

convert_text_resources_to_binary_on_export=true

[editor_plugins]

enabled=PoolStringArray( "res://addons/dialogic/plugin.cfg" )

[gui]

theme/use_hidpi=true

